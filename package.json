{
  "name": "expresso-api",
  "version": "1.0.0",
  "description": "An express ready setup app for faster API development",
  "main": "src/app.js",
  "type-js": "module",
  "type": "commonjs",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node -r dotenv/config src/app >> logs/log",
    "dev-js": "nodemon --experimental-specifier-resolution=node -r dotenv/config src/app.ts",
    "dev": "nodemon --experimental-specifier-resolution=node -r dotenv/config src/app.ts",
    "cli": "node expresso-cli || npx expresso-cli",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "express",
    "api",
    "mvc"
  ],
  "author": "Diether Marcelo",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/node": "^20.1.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
